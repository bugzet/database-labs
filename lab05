mirror_2=# CREATE TABLE transactions (
mirror_2(#     transaction_id BIGSERIAL PRIMARY KEY,
mirror_2(#     amount DECIMAL(15,2),
mirror_2(#     transaction_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP
mirror_2(# );
CREATE TABLE


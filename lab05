mirror_2=# CREATE TABLE course_enrollments (
mirror_2(#     student_id INTEGER,
mirror_2(#     course_id INTEGER,
mirror_2(#     semester VARCHAR(20),
mirror_2(#     enrollment_date DATE DEFAULT CURRENT_DATE,
mirror_2(#     grade CHAR(2),
mirror_2(#     PRIMARY KEY (student_id, course_id, semester)
mirror_2(# );
CREATE TABLE

